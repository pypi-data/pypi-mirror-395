                                ]
                            }
                        },
                        "domain-name": {
                            "fields": {
                                "value": [
                                    "hostinfo.domain"
                                ]
                            }
                        },
                        "x-oca-asset": {
                            "fields": {
                                "hostname": [
                                    "device.hostname"
                                ],
                                "device_id": [
                                    "device.device_id"
                                ],
                                "ip_refs[*].value": [
