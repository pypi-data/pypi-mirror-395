          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/wchk": {
      "get": {
        "summary": "Validate single whatsapp number",
        "description": "Enter a single phone number you want to validate.",
        "operationId": "validate_single_whatsapp_number",
        "parameters": [
          {
            "name": "phone",
            "in": "query",
            "required": true,
            "description": "The whatsapp number must be written as: number (including countrycode); do NOT include any non-number character, spaces, or anything which is not a number. Examples: of correct numbers are: 34123456789 (for spain) or 491234567890 (for Germany).",
            "schema": {
              "type": "number",
              "default": "34605797764",
              "enum": null
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/about": {
      "get": {
        "summary": "About info",
        "description": "This endpoint will return the *About* state of the WA number on the query.",
        "operationId": "about_info",
        "parameters": [
          {
            "name": "phone",
            "in": "query",
            "required": true,
            "description": "Example value: 34605797764",
            "schema": {
              "type": "number",
              "default": "34605797764",
              "enum": null
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/eb1000": {
