{
  "openapi": "3.0.0",
  "info": {
    "title": "Bulk Whatsapp Validator",
    "version": "1.0.0",
    "description": "RapidAPI: inutil-inutil-default/bulk-whatsapp-validator"
  },
  "servers": [
    {
      "url": "https://bulk-whatsapp-validator.p.rapidapi.com"
    }
  ],
  "paths": {
    "/isbiz": {
      "get": {
        "summary": "Is a business?",
        "description": "Requests to this endpoint will return `true` if the number is a Whatsapp for Business account, or `false` if it's not.",
        "operationId": "is_a_business?",
        "parameters": [
          {
            "name": "phone",
            "in": "query",
            "required": true,
            "description": "The whatsapp number must be written as: countrycode and number; do NOT include any non-number character, spaces, or anything which is not a number. Examples: of correct numbers are: 34123456789 (for spain) or 491234567890 (for Germany).",
            "schema": {
              "type": "number",
              "default": "34655719560",
              "enum": null
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/eb10": {
      "post": {
        "summary": "Validate up to 10",
        "description": "Enter an array of up to 10 numbers to validate.",
        "operationId": "validate_up_to_10",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/eb100": {
      "post": {
        "summary": "Validate up to 100",
        "description": "Enter an array of up to 100 numbers to validate.",
        "operationId": "validate_up_to_100",
        "parameters": [],
