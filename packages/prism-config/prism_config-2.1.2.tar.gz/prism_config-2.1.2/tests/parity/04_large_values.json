{
  "name": "large_value_support_16kb",
  "description": "Validates that configuration supports large values up to 16KB (Post-Quantum Cryptography key size)",
  "config": {
    "app": {
      "name": "pqc-test-app",
      "environment": "testing",
      "api_key": "LARGE_VALUE_16KB"
    },
    "database": {
      "host": "localhost",
      "port": 5432,
      "name": "test_db"
    }
  },
  "large_values": {
    "LARGE_VALUE_16KB": 16384
  },
  "options": {
    "apply_env": false,
    "resolve_secrets": false
  },
  "expected": {
    "app.name": "pqc-test-app",
    "app.environment": "testing",
    "app.api_key_length": 16384,
    "database.host": "localhost",
    "database.port": 5432,
    "database.name": "test_db"
  }
}
