{
  "openapi": "3.0.0",
  "info": {
    "title": "Qr Code By Api Ninjas",
    "version": "1.0.0",
    "description": "RapidAPI: apininjas/qr-code-by-api-ninjas"
  },
  "servers": [
    {
      "url": "https://qr-code-by-api-ninjas.p.rapidapi.com"
    }
  ],
  "paths": {
    "/v1/qrcode": {
      "get": {
        "summary": "/v1/qrcode",
        "description": "API Ninjas QR Code API endpoint. Returns a QRCode image binary specified by input parameters.",
        "operationId": "/v1/qrcode",
        "parameters": [
          {
            "name": "data",
            "in": "query",
            "required": true,
            "description": "data to encode in the QR code.",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          },
          {
            "name": "format",
            "in": "query",
            "required": true,
            "description": "image format to return. Must be one of the following: png, jpg, jpeg, eps, svg.",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          },
          {
            "name": "size",
            "in": "query",
            "required": false,
            "description": "size of the QR code image to generate. The output will be a square image with (size x size) dimensions.",
            "schema": {
              "type": "number",
              "default": "0",
              "enum": null
            }
          },
          {
            "name": "fg_color",
            "in": "query",
            "required": false,
            "description": "foreground color of the QR code. Must be a 6-digit hex color (e.g. 00ff00 for green). Default is 000000 (black)",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          },
          {
            "name": "bg_color",
            "in": "query",
            "required": false,
            "description": "background color of the QR code. Must be a 6-digit hex color (e.g. 00ff00 for green). Default is ffffff (white)",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "ApiAuth": {
        "type": "apiKey",
        "in": "header",
        "name": "X-RapidAPI-Key"
      }
    }
  },
  "security": [
    {
      "ApiAuth": []
    }
  ]
}