        "parameters": [
          {
            "name": "city",
            "in": "query",
            "required": true,
            "description": "City name.",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          },
          {
            "name": "state",
            "in": "query",
            "required": false,
            "description": "US state (for United States cities only).",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          },
          {
            "name": "country",
            "in": "query",
            "required": false,
            "description": "Country name, 2-letter ISO country code, or 3-letter ISO country code.",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/v1/reversegeocoding": {
      "get": {
        "summary": "/v1/reversegeocoding",
        "description": "API Ninjas Reverse Geocoding API endpoint.",
        "operationId": "/v1/reversegeocoding",
        "parameters": [
          {
            "name": "lat",
            "in": "query",
            "required": true,
            "description": "Latitude coordinate.",
            "schema": {
              "type": "number",
              "default": "47.6062",
              "enum": null
            }
          },
          {
            "name": "lon",
            "in": "query",
            "required": true,
            "description": "Longitude coordinate.",
            "schema": {
              "type": "number",
              "default": "-122.3321",
              "enum": null
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    }
  },
