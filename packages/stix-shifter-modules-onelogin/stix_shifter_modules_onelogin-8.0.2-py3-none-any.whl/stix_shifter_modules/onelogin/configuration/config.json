                                ],
                                "browser_fingerprint": [
                                    "browser_fingerprint"
                                ],
                                "notes": [
                                    "notes"
                                ],
                                "proxy_ip": [
                                    "proxy_ip"
                                ]
                            }
                        },
                        "x-onelogin-risk": {
                            "fields": {
                                "risk_score": [
                                    "risk_score"
                                ],
                                "risk_reasons": [
                                    "risk_reasons"
                                ],
                                "risk_cookie_id": [
                                    "risk_cookie_id"
                                ],
                                "error_description": [
                                    "error_description"
                                ]
                            }
                        }
                    },
                    "operators": {
                        "ComparisonExpressionOperators.And": "&",
                        "ComparisonComparators.Equal": "=",
                        "ObservationOperators.And": "or",
                        "ObservationOperators.Or": "or"
                    }
                }
            },
            "unmapped_fallback": {
                "type": "boolean",
                "default": false,
                "optional": true,
                "hidden": true
            },
            "stix_2.1": {
                "type": "boolean",
                "default": false,
                "optional": true,
                "hidden": true
            }
        }
    },
    "configuration": {
        "auth": {
            "type": "fields",
            "clientId": {
