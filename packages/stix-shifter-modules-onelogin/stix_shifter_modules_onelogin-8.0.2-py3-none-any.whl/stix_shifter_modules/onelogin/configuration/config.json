                        },
                        "directory_sync_run_id": {
                            "key": "x-onelogin-finding.directory_sync_run_id",
                            "object": "x-onelogin-finding"
                        },
                        "resource_type_id": {
                            "key": "x-onelogin-finding.resource_type_id",
                            "object": "x-onelogin-finding"
                        },
                        "browser_fingerprint": {
                            "key": "x-onelogin-finding.browser_fingerprint",
                            "object": "x-onelogin-finding"
                        },
                        "notes": {
                            "key": "x-onelogin-finding.notes",
                            "object": "x-onelogin-finding"
                        },
                        "created_at": {
                            "key": "x-ibm-finding.time_observed",
                            "object": "x-ibm-finding"
                        },
                        "risk_score": {
                            "key": "x-onelogin-risk.risk_score",
                            "object": "x-onelogin-risk"
                        },
                        "risk_reasons": {
                            "key": "x-onelogin-risk.risk_reasons",
                            "object": "x-onelogin-risk"
                        },
                        "risk_cookie_id": {
                            "key": "x-onelogin-risk.risk_cookie_id",
                            "object": "x-onelogin-risk"
                        },
                        "error_description": {
                            "key": "x-onelogin-risk.error_description",
                            "object": "x-onelogin-risk"
                        }
                    },
                    "from_stix_map": {
                        "user-account": {
                            "fields": {
                                "user_id": [
                                    "user_id"
                                ],
                                "display_name": [
                                    "user_name"
                                ],
