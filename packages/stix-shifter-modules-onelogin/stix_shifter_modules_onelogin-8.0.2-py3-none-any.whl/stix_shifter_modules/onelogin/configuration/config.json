{
    "connection": {
        "type": {
            "displayName": "OneLogin",
            "group": "onelogin",
            "type": "connectorType"
        },
        "region": {
            "type": "text",
            "default": "us"
        },
        "help": {
            "type": "link",
            "default": "data-sources.html"
        },
        "options": {
            "result_limit": {
                "default": 10000,
                "min": 1,
                "max": 10000000,
                "type": "number",
                "previous": "connection.resultSizeLimit"
            },
            "type": "fields",
            "async_call": {
                "type": "text",
                "hidden": true,
                "optional": true
            },
            "batch_size": {
                "default": 2000,
                "min": 1,
                "max": 10000,
                "type": "number",
                "optional": true,
                "hidden": true
            },
            "time_range": {
                "default": 5,
                "min": 1,
                "max": 10000,
                "type": "number",
                "previous": "connection.timerange",
                "nullable": true
            },
            "timeout": {
                "default": 30,
                "min": 1,
                "max": 3600,
                "hidden": true,
                "type": "number",
                "previous": "connection.timeoutLimit"
            },
            "dialects": {
                "type": "array",
                "hidden": true,
                "optional": true
            },
            "language": {
                "type": "string",
                "default": "stix",
                "optional": true,
                "hidden": true
            },
            "validate_pattern": {
                "type": "boolean",
                "optional": true,
                "hidden": true,
                "previous": "connection.validate_pattern"
            },
            "stix_validator": {
                "type": "boolean",
                "default": false,
                "optional": true,
                "hidden": true,
                "previous": "connection.stix_validator"
            },
            "mapping": {
                "type": "json",
                "optional": true,
                "previous": "connection.mapping",
                "default": {
                    "to_stix_map": {
                        "user_id": {
                            "object": "user-account",
                            "key": "user-account.user_id",
                            "transformer": "ToString"
                        },
                        "assuming_acting_user_id": {
                            "object": "user-account",
                            "key": "user-account.x_assuming_acting_user_id"
                        },
                        "actor_user_name": {
                            "object": "user-account",
                            "key": "user-account.x_actor_user_name"
                        },
                        "actor_user_id": {
                            "object": "user-account",
                            "key": "user-account.x_actor_user_id"
                        },
                        "account_id": {
                            "object": "user-account",
                            "key": "user-account.x_account_id"
                        },
                        "user_name": {
                            "key": "user-account.display_name",
                            "object": "user-account"
                        },
                        "client_id": {
                            "key": "x-onelogin-finding.client_id",
                            "object": "x-onelogin-finding"
                        },
                        "directory_id": {
                            "key": "x-onelogin-finding.directory_id",
                            "object": "x-onelogin-finding"
                        },
                        "event_type_id": {
                            "key": "x-onelogin-finding.event_type_id",
                            "object": "x-onelogin-finding"
                        },
                        "id": {
                            "key": "x-onelogin-finding.unique_id",
                            "object": "x-onelogin-finding"
                        },
                        "resolution": {
                            "key": "x-onelogin-finding.resolution",
                            "object": "x-onelogin-finding"
                        },
                        "ipaddr": {
                            "key": "ipv4-addr.value",
                            "object": "ipv4-addr"
                        },
                        "proxy_ip": {
                            "key": "x-onelogin-finding.proxy_ip",
                            "object": "x-onelogin-finding"
                        },
                        "policy_id": {
                            "key": "x-ibm-finding.policy_id",
                            "object": "x-ibm-finding"
                        },
                        "policy_name": {
                            "key": "x-ibm-finding.name",
                            "object": "x-ibm-finding"
                        },
                        "role_id": {
                            "key": "x-onelogin-finding.role_id",
                            "object": "x-onelogin-finding"
                        },
                        "app_id": {
                            "key": "x-onelogin-finding.app_id",
                            "object": "x-onelogin-finding"
                        },
                        "custom_message": {
                            "key": "x-onelogin-finding.custom_message",
                            "object": "x-onelogin-finding"
                        },
                        "role_name": {
                            "key": "x-onelogin-finding.role_name",
