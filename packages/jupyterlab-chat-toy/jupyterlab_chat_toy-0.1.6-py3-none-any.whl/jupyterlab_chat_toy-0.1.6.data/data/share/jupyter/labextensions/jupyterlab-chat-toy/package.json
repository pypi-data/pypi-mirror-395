{
  "name": "jupyterlab-chat-toy",
  "version": "0.1.6",
  "description": "AI Chat Extension for JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/your-username/jupyterlab-chat",
  "bugs": {
    "url": "https://github.com/your-username/jupyterlab-chat/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Your Name",
  "files": [
    "lib/**/*.{d.ts,js,js.map}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,woff,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/jupyterlab-chat"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:lib": "tsc",
    "build:labextension:dev": "jupyter labextension build --development=True --source-map=True .",
    "build:labextension:prod": "jupyter labextension build .",
    "build:prod": "jlpm run build:lib && jlpm run build:labextension:prod",
    "clean": "jlpm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf jupyterlab_chat_toy/labextension",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "jlpm run build",
    "prepare": "jlpm run clean && jlpm run build:prod",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyterlab/application": "^3.2.9",
    "@jupyterlab/apputils": "^3.2.9",
    "@jupyterlab/launcher": "^3.2.9",
    "@jupyterlab/notebook": "^3.2.9",
    "@jupyterlab/settingregistry": "^3.2.9",
    "@lumino/widgets": "^1.37.2",
    "react": "^17.0.2",
    "react-markdown": "^8.0.3",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.2.9",
    "@types/react": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "~4.1.3"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "jupyterlab_chat_toy/labextension",
    "schemaDir": "schema",
    "sharedPackages": {
      "react": {
        "bundled": false,
        "singleton": true
      },
      "react-dom": {
        "bundled": false,
        "singleton": true
      }
    },
    "_build": {
      "load": "static/remoteEntry.88d23cb712b205706c69.js",
      "extension": "./extension",
      "style": "./style"
    }
  }
}
