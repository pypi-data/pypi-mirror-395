include README.md
include LICENSE
include NOTICE
include qmcp/config_template.toml
recursive-include qmcp/parseq *.py *.q *.md
recursive-include qmcp/qpython *.py *.pyx
exclude qmcp/qpython/display.py
exclude qmcp/qython/*.py
include qmcp/qython/docs_api.py
include qmcp/qython/q_utils.py
include qmcp/qython/qython.q
recursive-include qmcp/qython/docs *.md
recursive-exclude qmcp/qython/array *
recursive-exclude qmcp/qython/atoms *
recursive-exclude qmcp/qython/expression *
recursive-exclude qmcp/qython/qythonsql *
recursive-exclude tests_* *
recursive-exclude private_tests* *
prune tests_qython
prune tests_qpython
prune private_tests