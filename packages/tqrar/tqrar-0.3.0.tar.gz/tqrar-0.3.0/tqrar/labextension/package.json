{
  "name": "tqrar",
  "version": "0.2.0",
  "description": "TQRAR - AI-powered assistant for JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "ai",
    "assistant",
    "tqrar"
  ],
  "homepage": "https://github.com/tqrar/tqrar",
  "bugs": {
    "url": "https://github.com/tqrar/tqrar/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Arsalan Javed",
    "email": "arsalanjaved2010@gmail.com"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "schema/*.json",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/tqrar/tqrar.git"
  },
  "scripts": {
    "build": "jlpm build:css && jlpm build:lib && jlpm build:labextension:dev",
    "build:prod": "jlpm clean && jlpm build:css && jlpm build:lib:prod && jlpm build:labextension",
    "build:css": "tailwindcss -i ./style/tailwind.css -o ./style/index.css --minify",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc --sourceMap",
    "build:lib:prod": "tsc",
    "clean": "jlpm clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:lintcache": "rimraf .eslintcache .stylelintcache",
    "clean:labextension": "rimraf tqrar/labextension tqrar/_version.py",
    "clean:all": "jlpm clean:lib && jlpm clean:labextension && jlpm clean:lintcache",
    "install:extension": "jlpm build",
    "dev": "jlpm build && jupyter labextension develop . --overwrite",
    "watch": "run-p watch:css watch:src watch:labextension",
    "watch:css": "tailwindcss -i ./style/tailwind.css -o ./style/index.css --watch",
    "watch:src": "tsc -w --sourceMap",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyterlab/application": "^4.0.0",
    "@jupyterlab/apputils": "^4.0.0",
    "@jupyterlab/cells": "^4.0.0",
    "@jupyterlab/codeeditor": "^4.0.0",
    "@jupyterlab/codemirror": "^4.0.0",
    "@jupyterlab/coreutils": "^6.0.0",
    "@jupyterlab/docregistry": "^4.0.0",
    "@jupyterlab/notebook": "^4.0.0",
    "@jupyterlab/rendermime": "^4.0.0",
    "@jupyterlab/services": "^7.0.0",
    "@jupyterlab/settingregistry": "^4.0.0",
    "@jupyterlab/statedb": "^4.0.0",
    "@jupyterlab/ui-components": "^4.0.0",
    "@lumino/algorithm": "^2.0.0",
    "@lumino/commands": "^2.0.0",
    "@lumino/coreutils": "^2.0.0",
    "@lumino/disposable": "^2.0.0",
    "@lumino/messaging": "^2.0.0",
    "@lumino/signaling": "^2.0.0",
    "@lumino/widgets": "^2.0.0",
    "@radix-ui/react-switch": "^1.0.3",
    "clsx": "^2.1.1",
    "openai": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.0",
    "@types/node": "^24.10.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "stylelint": "^15.10.1",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-csstree-validator": "^3.0.0",
    "stylelint-prettier": "^4.0.0",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.1.6"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "tqrar/labextension",
    "schemaDir": "schema",
    "_build": {
      "load": "static/remoteEntry.9cbf49ce057c7bac45ab.js",
      "extension": "./extension",
      "style": "./style"
    }
  }
}
