{
  "openapi": "3.0.0",
  "info": {
    "title": "Biztoc",
    "version": "1.0.0",
    "description": "RapidAPI: thma/biztoc"
  },
  "servers": [
    {
      "url": "https://biztoc.p.rapidapi.com"
    }
  ],
  "paths": {
    "/news/source/{source_id}": {
      "get": {
        "summary": "News / Source",
        "description": "Get the latest news stories for the respective `{source_id}` in chronological order.",
        "operationId": "news_/_source",
        "parameters": [
          {
            "name": "source_id",
            "in": "query",
            "required": true,
            "description": "Example value: bbc",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/sources": {
      "get": {
        "summary": "Sources",
        "description": "Lists all currently available `{source_id}`s. Equivalent of news blocks on biztoc.com. Note that certain proprietary blocks like Twitter or Stock Tickers are not available.",
        "operationId": "sources",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/news/topics": {
      "get": {
        "summary": "News / Topics",
        "description": "Get trending topic clusters and their respective stories, ordered by a proprietary popularity algorithm. Akin to the /topics view on biztoc.com. The topics and stories will change every hour.",
        "operationId": "news_/_topics",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/search": {
      "get": {
        "summary": "Search",
        "description": "Search across the entire news index from the last 14 days. Note that no search operators are supported via the API so all provided terms are considered required.",
        "operationId": "search",
        "parameters": [
          {
            "name": "q",
            "in": "query",
            "required": true,
            "description": "Example value: apple",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/news/latest": {
      "get": {
        "summary": "News / Latest",
        "description": "Get the latest news stories in chronological order, across all pages. Akin to the /wire view on biztoc.com.",
        "operationId": "news_/_latest",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "ApiAuth": {
        "type": "apiKey",
        "in": "header",
        "name": "X-RapidAPI-Key"
      }
    }
  },
  "security": [
    {
      "ApiAuth": []
    }
  ]
}