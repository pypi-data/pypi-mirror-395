# NanoHub Citation Manager Environment Configuration
# Copy this file to .env and fill in your actual credentials

# NanoHub API Configuration
NANOHUB_URL=https://nanohub.org/api
NANOHUB_TOKEN=your_nanohub_api_token_here

# LLM API Configuration (OpenWebUI/Purdue GenAI)
OPENWEBUI_KEY=your_openwebui_api_key_here
OPENWEBUI_URL=your_openwebui_url

# LLM Model to use
LLM_MODEL=gpt-oss:120b
