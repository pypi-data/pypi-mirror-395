[project]
name = "{project_name}"
version = "0.1.0"
description = "A FastAPI application"
authors = [
    {{email = "{author_email}"}},
]
requires-python = ">=3.12"
dependencies = [
    "fastapi>=0.115.0",
    "hypercorn[uvloop]>=0.17.0",
    "pytest>=8.0.0",
    "httpx>=0.27.0",
]

[tool.uv]
dev-dependencies = []

[build-system]
requires = ["hatchling"]
build-backend = "hatchling.build"
