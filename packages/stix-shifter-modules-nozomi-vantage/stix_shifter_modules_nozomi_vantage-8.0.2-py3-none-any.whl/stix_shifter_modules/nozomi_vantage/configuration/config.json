                                ],
                                "parent_ref.binary_ref.parent_directory_ref.path": [
                                    "properties/process/ancestry"
                                ]
                            }
                        },
                        "user-account": {
                            "fields": {
                                "user_id": [
                                    "properties/process/user"
                                ]
                            }
                        },
                        "directory": {
                            "fields": {
                                "path": [
                                    "properties/process/image_path"
                                ]
                            }
                        },
                        "x-ibm-finding": {
                            "fields": {
                                "alert_id": [
                                    "id"
                                ],
                                "finding_type": [
                                    "threat_name"
                                ],
                                "name": [
                                    "type_name"
                                ],
                                "description": [
                                    "description"
                                ],
                                "time_observed": [
                                    "time"
                                ],
                                "start": [
