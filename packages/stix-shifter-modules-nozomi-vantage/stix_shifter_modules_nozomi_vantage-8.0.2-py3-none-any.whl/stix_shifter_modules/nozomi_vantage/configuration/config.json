                    "from_stix_map": {
                        "ipv4-addr": {
                            "fields": {
                                "value": [
                                    "ip_dst",
                                    "ip_src"
                                ]
                            }
                        },
                        "ipv6-addr": {
                            "fields": {
                                "value": [
                                    "ip_dst",
                                    "ip_src"
                                ]
                            }
                        },
                        "network-traffic": {
                            "fields": {
                                "src_ref.value": [
                                    "ip_src"
                                ],
                                "dst_ref.value": [
                                    "ip_dst"
                                ],
                                "dst_port": [
                                    "port_dst"
                                ],
                                "src_port": [
                                    "port_src"
                                ],
                                "protocols[*]": [
                                    "protocol",
                                    "transport_protocol"
                                ]
                            }
                        },
                        "mac-addr": {
                            "fields": {
                                "value": [
                                    "mac_src",
                                    "mac_dst"
                                ]
                            }
                        },
                        "file": {
                            "fields": {
                                "name": [
                                    "properties/details_yara_file/value",
                                    "properties/process/image_path"
                                ],
                                "hashes.'SHA-256'": [
                                    "properties/details_hash_SHA256/value",
                                    "properties/process/image_hash_sha256"
                                ],
                                "hashes.'SHA-1'": [
                                    "properties/details_hash_SHA1/value"
                                ],
                                "hashes.MD5": [
                                    "properties/details_hash_MD5/value"
                                ],
                                "size": [
                                    "properties/details_file_size/value"
                                ],
                                "parent_directory_ref.path": [
                                    "properties/process/image_path"
                                ]
                            }
                        },
                        "process": {
                            "fields": {
                                "pid": [
