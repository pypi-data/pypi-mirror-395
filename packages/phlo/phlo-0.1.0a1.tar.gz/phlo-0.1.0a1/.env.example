# Postgres
POSTGRES_USER=lake
POSTGRES_PASSWORD=lakepass
POSTGRES_DB=lakehouse
POSTGRES_PORT=10000

# MinIO
MINIO_HOST=minio
MINIO_ROOT_USER=minio
MINIO_ROOT_PASSWORD=minio999
MINIO_CONSOLE_PORT=10002
MINIO_API_PORT=10001

# Nessie (Git-like data catalog)
NESSIE_VERSION=0.105.5
NESSIE_PORT=10003

# Trino (Query Engine)
TRINO_VERSION=477
TRINO_PORT=10005

# Iceberg
ICEBERG_WAREHOUSE_PATH=s3://lake/warehouse
ICEBERG_STAGING_PATH=s3://lake/stage
ICEBERG_NESSIE_REF=main

# Nessie Branch Management
BRANCH_RETENTION_DAYS=7
BRANCH_RETENTION_DAYS_FAILED=14
AUTO_PROMOTE_ENABLED=true

# Validation Gates
FRESHNESS_BLOCKS_PROMOTION=false
PANDERA_CRITICAL_LEVEL=error

# Validation Retry
VALIDATION_RETRY_ENABLED=true
VALIDATION_RETRY_MAX_ATTEMPTS=3
VALIDATION_RETRY_DELAY_SECONDS=300

# Freshness Thresholds
GLUCOSE_FRESHNESS_HOURS=24
GITHUB_EVENTS_FRESHNESS_HOURS=24
GITHUB_STATS_FRESHNESS_HOURS=48

# Dagster
DAGSTER_PORT=10006

