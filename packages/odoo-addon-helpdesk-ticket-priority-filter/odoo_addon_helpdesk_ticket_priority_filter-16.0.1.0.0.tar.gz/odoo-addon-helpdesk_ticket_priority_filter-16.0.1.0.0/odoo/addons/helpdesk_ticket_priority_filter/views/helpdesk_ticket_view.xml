<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="helpdesk_ticket_priority_filter.helpdesk_ticket_search_view_priority_inherit" model="ir.ui.view">
        <field name="name">helpdesk.ticket.search.view.priority.inherit</field>
        <field name="model">helpdesk.ticket</field>
        <field name="inherit_id" ref="helpdesk_mgmt.helpdesk_ticket_view_search"/>
        <field name="arch" type="xml">
            <filter name="high_priority" position="replace">
                <filter
                    string="High Priority"
                    name="high_priority"
                    domain="[('is_high_priority','=',True)]"
                />
            </filter>
        </field>
    </record>
</odoo>