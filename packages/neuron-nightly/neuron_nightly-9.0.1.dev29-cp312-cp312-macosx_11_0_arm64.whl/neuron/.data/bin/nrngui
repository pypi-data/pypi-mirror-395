#!/usr/bin/env bash

ARCH=arm64
MODSUBDIR=arm64

if test "x${NRNHOME}" = x ; then
	prefix=/var/folders/n2/cx7hccrs4kqgf70kqhksvr100000gn/T/tmpvjz85qi0/wheel/platlib
	exec_prefix=/var/folders/n2/cx7hccrs4kqgf70kqhksvr100000gn/T/tmpvjz85qi0/wheel/platlib
	NRNBIN=${exec_prefix}/bin/
else
    prefix="$NRNHOME"
    exec_prefix="${prefix}"
	NRNBIN="${exec_prefix}/bin/"
fi

if test "${NRNHOME_EXEC}" != "" ; then
	exec_prefix="${NRNHOME_EXEC}"
	NRNBIN="${exec_prefix}/bin/"
fi

NRNIV="${NRNBIN}nrniv"
NEURONHOME="${prefix}/share/nrn"
export NEURONHOME

if [ -x ${MODSUBDIR}/special ] ; then
	program="./${MODSUBDIR}/special"
else
	program="${NRNIV}"
fi

exec "$program" "$NEURONHOME/lib/hoc/nrngui.hoc" "$@" -

