        "summary": "Property Details",
        "description": "Get property details. Requires the property ID.",
        "operationId": "property_details",
        "parameters": [
          {
            "name": "id",
            "in": "query",
            "required": true,
            "description": "Property Id. 'Id' tag from properties listing. It should have this format: '1_123456789'. NOT to be confused with the 'propertyId', which doesn't include the first number. The first number indicates the operation. 1 is for sale, 3 is for rent.",
            "schema": {
              "type": "string",
              "default": null,
              "enum": null
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
